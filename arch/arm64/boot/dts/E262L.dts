/* zhangkaiyuan@wind-mobi.com 20170327 begin */
&accdet {
		accdet-mic-vol = <7>;
		headset-mode-setting = <0x500 0x500 1 0x3F0 0x800 0x800 0x200>;
		accdet-plugout-debounce = <20>;
		/*1:ACC mode, 2:low cost without in bias, 6:low cost with in bias*/
		accdet-mic-mode = <1>;
		/*0--MD_MAX--UP_MAX--DW_MAX*/
		headset-three-key-threshold = <0 80 220 500>;
		/*0--MD_MAX--VOICE_MAX--UP_MAX--DW_MAX*/
		headset-four-key-threshold = <0 59 123 195 450>;
		pinctrl-names = "default", "state_eint_as_int","headphone_high","headphone_low","headphone2_high","headphone2_low";
		pinctrl-0 = <&ACCDET_pins_default>;
		pinctrl-1 = <&ACCDET_pins_eint_int>;
		pinctrl-2 = <&ACCDET_pins_headphone_high>;
		pinctrl-3 = <&ACCDET_pins_headphone_low>;
		pinctrl-4 = <&ACCDET_pins_headphone2_high>;
		pinctrl-5 = <&ACCDET_pins_headphone2_low>;
		status = "okay";
};
/* ACCDET GPIO Standardization ACC mode use */
&pio {
	ACCDET_pins_default:eint2default {
	};
	ACCDET_pins_eint_int:eint@2 {
		pins_cmd_dat {
			pins = <PINMUX_GPIO2__FUNC_GPIO2>;
			slew-rate = <0>;
			bias-disable;
		};
	};
	 ACCDET_pins_headphone_high: headhigh{	
	         pins_cmd_dat {	
		         pins = <PINMUX_GPIO83__FUNC_GPIO83>;	
		         slew-rate = <1>;	
	                 output-high;	
	         };	
	 };	
	 ACCDET_pins_headphone_low: headlow {	
	          pins_cmd_dat {	
	                  pins = <PINMUX_GPIO83__FUNC_GPIO83>;	
		          slew-rate = <1>;	
		          output-low;	
		  };
	 };	
	ACCDET_pins_headphone2_high: headhigh2{	
	         pins_cmd_dat {	
		           pins = <PINMUX_GPIO85__FUNC_GPIO85>;	
			   slew-rate = <1>;	
	                   output-high;	
		 };	
	};	
	 ACCDET_pins_headphone2_low: headlow2 {	
	          pins_cmd_dat {	
	                  pins = <PINMUX_GPIO85__FUNC_GPIO85>;	
			  slew-rate = <1>;	
		          output-low;	
		  };
	 };
};
