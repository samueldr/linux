/* FINGERPRINT start */

&finger{
	reg = <0>;
/*	spi-max-frequency = <8000000>;  */
	interrupt-parent = <&eintc>;
	interrupts = <11 IRQ_TYPE_EDGE_RISING>;
	debounce = <11 0>;
	vdd_ldo_enable = <0>;
	vio_ldo_enable = <0>;
	config_spi_pin = <1>;
	netlink-event = <26>;			
	pinctrl-names = "default", "fingerprint_irq","miso_spi", "miso_pullhigh", "miso_pulllow","reset_high", "reset_low";
	pinctrl-0 = <&fingerprint_default>;
	pinctrl-1 = <&fingerprint_irq>;
	pinctrl-2 = <&fingerprint_miso_spi>;
	pinctrl-3 = <&fingerprint_miso_pullhigh>;
	pinctrl-4 = <&fingerprint_miso_pulllow>;
	pinctrl-5 = <&fingerprint_reset_high>;
	pinctrl-6 = <&fingerprint_reset_low>;
			status = "okay";
};




&spi0 {
	#address-cells = <1>;
	#size-cells = <0>;

	fingerprint@0 {
		compatible = "goodix,goodix-fp";
		reg = <0>;
		spi-max-frequency = <8000000>;
		netlink-event = <26>;
		status = "okay";
	};
};

&goodix_fp {
	pinctrl-names = "default", "fingerprint_irq","miso_spi", "miso_pullhigh", "miso_pulllow","reset_high", "reset_low";
	pinctrl-0 = <&fingerprint_default>;
	pinctrl-1 = <&fingerprint_irq>;
	pinctrl-2 = <&fingerprint_miso_spi>;
	pinctrl-3 = <&fingerprint_miso_pullhigh>;
	pinctrl-4 = <&fingerprint_miso_pulllow>;
	pinctrl-5 = <&fingerprint_reset_high>;
	pinctrl-6 = <&fingerprint_reset_low>;
	status = "okay";
};

&pio {
	fingerprint_default: state_default {
	};

	fingerprint_irq: fingerprint_irq {
		pins_cmd_dat {
			pins = <PINMUX_GPIO11__FUNC_GPIO11>;
			slew-rate = <0>;
			bias-disable;
		};
	};
	
	fingerprint_miso_spi: state_miso_spi {
		pins_cmd_dat {
			pins = <PINMUX_GPIO95__FUNC_SPI0_MI>;
		};
	};

	fingerprint_miso_pullhigh: state_miso_pullhigh {
		pins_cmd_dat {
			pins = <PINMUX_GPIO95__FUNC_GPIO95>;
			slew-rate = <1>;
			output-high;
		};
	};

	fingerprint_miso_pulllow: state_miso_pulllow {
		pins_cmd_dat {
			pins = <PINMUX_GPIO95__FUNC_GPIO95>;
			slew-rate = <1>;
			output-low;
		};
	};
	
	fingerprint_reset_high: state_reset_high {
		pins_cmd_dat {
			pins = <PINMUX_GPIO51__FUNC_GPIO51>;
			slew-rate = <1>;
			output-high;
		};
	};

	fingerprint_reset_low: state_reset_low {
		pins_cmd_dat {
			pins = <PINMUX_GPIO51__FUNC_GPIO51>;
			slew-rate = <1>;
			output-low;
		};
	};
};
//hebiao@wind-mobi.com 20161020 end
