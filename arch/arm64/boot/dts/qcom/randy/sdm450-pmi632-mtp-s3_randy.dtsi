&mdss_dsi0 {
	pinctrl-0 = <&mdss_dsi_active &mdss_te_active>;
	pinctrl-1 = <&mdss_dsi_suspend &mdss_te_suspend>;
};

&soc {
	gpio_keys {
		compatible = "gpio-keys";
		input-name = "gpio-keys";
		pinctrl-names = "default";
		pinctrl-0 = <&gpio_key_active>;

		/delete-node/ camera_focus;
		/delete-node/ camera_snapshot;
		vol_up {
			label = "volume_up";
			gpios = <&tlmm 85 0x1>;
			linux,input-type = <1>;
			linux,code = <115>;
			debounce-interval = <15>;
			linux,can-disable;
			gpio-key,wakeup;
		};
		vol_down {
			label = "volume_down";
			gpios = <&tlmm 66 0x1>;
			linux,input-type = <1>;
			linux,code = <114>;
			debounce-interval = <15>;
			linux,can-disable;
			gpio-key,wakeup;
		};
	};

	zte_misc {
		compatible = "zte-misc";
	};
};
&gpio_key_active {
	mux {
		pins = "gpio85", "gpio66";
		function = "gpio";
	};
	config {
		pins = "gpio85", "gpio66";
		drive-strength = <2>;
		bias-pull-up;
	};
};
&gpio_key_suspend {
	mux {
		pins = "gpio85", "gpio66";
		function = "gpio";
	};
	config {
		pins = "gpio85", "gpio66";
		drive-strength = <2>;
		bias-pull-up;
	};
};

&tlmm {
/* zte typec */
	tlmm_gpio_typec {
		typec_intpin_cfg: typecintpincfg {
				mux {
					pins = "gpio36";
					function = "gpio";
				};
				config {
					pins = "gpio36";
					drive-strength = <2>;
					bias-disable;
				};
		};
		typec_enpin_output0: enpin_output0 {
				mux {
					pins = "gpio140";
					function = "gpio";
				};
				config {
					pins = "gpio140";
					drive-strength = <2>;
					bias-disable;
					output-low;
				};
		};
		typec_enpin_output1: enpin_output1 {
				mux {
					pins = "gpio140";
					function = "gpio";
				};
				config {
					pins = "gpio140";
					drive-strength = <2>;
					bias-disable;
					output-high;
				};
		};
	};
/*zte typec */
};

