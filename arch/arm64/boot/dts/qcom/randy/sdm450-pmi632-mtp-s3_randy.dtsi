&pmi632_charger {
	qcom,usb-icl-ua = <1500000>;
	qcom,thermal-mitigation
		= <1500000 1300000 1000000 600000
			300000 150000>;
	qcom,auto-recharge-soc = <99>;
	qcom,hvdcp-disable;
	qcom,chg-term-src = <1>;
	qcom,chg-term-current-ma = <(-150)>;
	qcom,jeita-hysteresis-support;

	/* COOL = 10 DegC, WARM = 43 DegC */
	qcom,jeita-soft-thresholds = <0x3772 0x141e>;
	/* COLD = 0 DegC, HOT = 45 DegC */
	qcom,jeita-hard-thresholds = <0x45f5 0x12d2>;

	/* COLD = 2 DegC, HOT = 45 DegC */
	qcom,jeita-cold-hysteresis-thresholds = <0x430E 0x12D2>;
	/* COOL = 12 DegC, WARM = 43 DegC */
	qcom,jeita-cool-hysteresis-thresholds = <0x349E 0x141E>;
	/* COOL = 10 DegC, WARM = 42 DegC */
	qcom,jeita-warm-hysteresis-thresholds = <0x3772 0x14CD>;
	/* COLD = 0 DegC, HOT = 44 DegC */
	qcom,jeita-hot-hysteresis-thresholds = <0x45F5 0x1375>;
};

/*typec*/
&usb3 {
	extcon = <&pmi632_charger>, <&pmi632_charger>, <0>,
		<&pmi632_charger>, <&extcon_usb1>;
	vbus_dwc3-supply = <&smb5_vbus>;
};

&soc {
	extcon_usb1: extcon_usb1 {
		compatible = "linux,extcon-usb-gpio";
		id-gpio = <&tlmm 9 0>;
/*		pinctrl-names = "default";  */
/*		pinctrl-0 = <&usb_id_default>;*/
	};
};

/*typec*/

&pmi632_qg {
	qcom,qg-iterm-ma = <200>;
	qcom,shutdown_soc_threshold = <30>;
	qcom,ignore-shutdown-soc-secs = <43200>;
	qcom,shutdown-temp-diff = <150>;
	qcom,cl-feedback-on;
	qcom,cl-min-start-soc = <1>;
	qcom,cl-max-start-soc = <30>;
	qcom,cl-max-temp = <430>;
	qcom,cl-max-increment = <30>;
	qcom,cl-max-decrement = <10>;
	qcom,tcss-enable;
};

