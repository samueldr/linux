&i2c_8 {
	status = "okay";
	pinctrl-names = "i2c_active", "i2c_sleep";
	pinctrl-0 = <&i2c_8_active>;
	pinctrl-1 = <&i2c_8_sleep>;
	tfa9897@34 {
		compatible = "nxp,tfa98xx";
		reg = <0x34>;
		reset-gpio = <&tlmm 8 0x0>;
	};
};

&tlmm {
	i2c_8 {
		i2c_8_active: i2c_8_active {
			/* active state */
			mux {
				pins = "gpio98", "gpio99";
				function = "blsp_i2c8";
			};

			config {
				pins = "gpio98", "gpio99";
				drive-strength = <2>;
				bias-disable;
			};
		};

		i2c_8_sleep: i2c_8_sleep {
			/* suspended state */
			mux {
				pins = "gpio98", "gpio99";
				function = "gpio";
			};

			config {
				pins = "gpio98", "gpio99";
				drive-strength = <2>;
				bias-disable;
				input-enable;
			};
		};
	};

	/* SAR sensor INT */
	sx932x_int_active: sx932x_int_active {
		mux {
			pins = "gpio63";
			function = "gpio";
		};
		config {
			pins = "gpio63";
			drive-strength = <2>;
			bias-pull-up;
			input-enable;
		};
		};
		sx932x_int_suspend: sx932x_int_suspend {
		mux {
			pins = "gpio63";
			function = "gpio";
		};
		config {
			pins = "gpio63";
			drive-strength = <2>;
			bias-pull-up;
			input-enable;
		};
	};
};

&i2c_5 {
	status = "okay";
	sx932x:sar@28 {
		compatible = "Semtech,sx932x";
		reg = <0x28>;
		interrupt-parent = <&tlmm>;
		interrupts = <63 2>;
		interrupt-names = "sx932x_irq";
		pinctrl-names = "sx932x_active", "sx932x_sleep";
		pinctrl-0 = <&sx932x_int_active>;
		pinctrl-1 = <&sx932x_int_suspend >;
		Semtech,nirq-gpio = <&tlmm 63 0x00>;
