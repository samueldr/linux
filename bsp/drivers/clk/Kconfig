menu "Clock Drivers"
	depends on AW_BSP

config AW_CCU
	tristate "Clock Support for Allwinner SoCs"
	depends on AW_BSP
	default y
	select RESET_CONTROLLER
	select AW_RTC_CCU

config AW_CCU_DEBUG
	tristate "DebugFS representation of clock tree"
	default AW_CCU
	depends on DEBUG_FS

config AW_SUN8IW11_CCU
	tristate "SUN8IW11 CCU"
	default ARCH_SUN8IW11
	depends on AW_CCU

